#include<stdio.h>
#include<string.h>
#include<ctype.h>
char s[1000],st[1000][1000];
 
int main(){
    while(scanf("%s",s)!=-1){
        int n=strlen(s);
        int cnt=0;
        int flag=0;
        for(int i=0;i<n;){
            if(s[i]=='.'){
                i++;
                if(s[i]=='.'){
                    if(cnt==0){
                        flag=1;break;
                    }
                    cnt--;
                    i++;
                }
                while(s[i]=='/'||s[i]=='\\') {
                    i++;
                }
            }
            if(isalpha(s[i])||isdigit(s[i])){
                int tmp=0;
                while(isalpha(s[i])||isdigit(s[i])||s[i]=='.'){
                    if(s[i]=='.'){i++;continue;}
                    st[cnt][tmp++]=s[i++];
                }
                st[cnt++][tmp]='\0';
                while(s[i]=='/'||s[i]=='\\') {
                    i++;
                }
            }
            while(s[i]=='/'||s[i]=='\\') {
                i++;
            }
        }
        if(flag){printf("Value Error\n");
        continue;}
        if(s[0]=='/'||s[0]=='\\') printf("/");
        for(int i=0;i<cnt-1;i++) printf("%s/",st[i]);
        if(cnt) printf("%s\n",st[cnt-1]);
        else printf("\n");
    }
    return 0;   
}


