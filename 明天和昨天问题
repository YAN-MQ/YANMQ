#include<stdio.h>

int isLeap(int year){
    if(!(year%100)){
        if(year%400) return 0;
        return 1;
    }
    else{
        if(year%4) return 0;
        return 1;
    }
}
int main()
{
    int days[]={
	     31,31,28,31,30,31,30,31,31,30,31,30,31
	    };
    int year,month,day;
    scanf("%d %d %d",&year,&month,&day);
    int dd=0;
    if(isLeap(year)) days[2]++;
    if(day==1){
        printf("%d %d %d\n",(month==1?year-1:year),(month==1?12:month-1),days[month-1]);
        printf("%d %d %d",year,month,2);
    }
    else if(day==days[month]){
        printf("%d %d %d\n",year,month,day-1);
        printf("%d %d %d",(month==12?year+1:year),(month==12?1:month+1),1);
    }
    else {
        printf("%d %d %d\n",year,month,day-1);
        printf("%d %d %d",year,month,day+1);
    }
}
